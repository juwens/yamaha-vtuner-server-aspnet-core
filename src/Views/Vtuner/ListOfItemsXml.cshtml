@using System.Linq;
@model IEnumerable<VtnrNetRadioServer.Repositories.ItemContainer>
<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<ListOfItems>
    <ItemCount>@Model.Count()</ItemCount>
    @foreach (var m in Model)
    {
    <Item> <!-- minimal example -->
        <ItemType>@m.Item.ItemType</ItemType>
        <StationName>@m.Item.StationName</StationName>
        <StationUrl>@m.Item.StationUrl</StationUrl>
    </Item>
    }
</ListOfItems>